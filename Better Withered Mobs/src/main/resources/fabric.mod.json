{
  "schemaVersion": 1,
  "id": "better_withered_mobs",
  "version": "${version}",
  "name": "Better Withered Mobs",
  "description": "This mod adds Withered Bones, Withered Bone Meal, Withered Bone Blocks, a Potion of Decay, and a Withering enchantment! Wither Skeletons spawn with swords with the Withering enchantment of a random level (This can be disabled in the config). Withered mobs no longer drop regular Bones and only drop Withered Bones (This can be disabled in the config).",
  "authors": [
    "SmushyTaco"
  ],
  "contact": {
    "homepage": "https://www.smushytaco.com/",
    "issues": "https://www.curseforge.com/minecraft/mc-mods/better-wither-skeletons/issues/",
    "sources": "https://github.com/SmushyTaco/Better-Withered-Mobs"
  },
  "license": "GNU Lesser General Public License version 3 (LGPLv3)",
  "icon": "assets/better_withered_mobs/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.smushytaco.better_withered_mobs.BetterWitheredMobs"
      }
    ],
    "preLaunch": [
      {
        "adapter": "kotlin",
        "value": "com.smushytaco.better_withered_mobs.BetterWitheredMobsPreLaunch"
      }
    ],
    "modmenu": [
      "com.smushytaco.better_withered_mobs.configuration_support.ModMenuIntegration"
    ]
  },
  "mixins": [
    "better_withered_mobs.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabricloader}",
    "fabric-api": ">=${fabric_api}",
    "fabric-language-kotlin": ">=${fabric_language_kotlin}",
    "minecraft": ">=${minecraft}",
    "java": ">=${java}",
    "cloth-config2": ">=${clothconfig}",
    "com_github_llamalad7_mixinextras": ">=${mixinextras}"
  },
  "suggests": {
    "modmenu": ">=${modmenu}"
  }
}